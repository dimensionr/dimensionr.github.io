@import 'colors'
@import 'main'


p
	font-size: 14px
	padding: 0
	margin: 0
h6
	position: relative;
	text-transform: uppercase;
	font-size: 14px
	font-weight: bold
	padding: 10px 20px

.ii
	text-transform: uppercase;
	// font-weight: bold
	// color: rgb(32, 221, 62)
	color color_ii
	text-shadow: 0 0 1px color_2

li::before
	content: ''

body
	color color_body
	background: rgb(28, 26, 26)

// Copyright
.copyright
	text-align: center;

.copyright p
	margin: 0
	padding: 0
	color rgba(color_2, .5)


// TABLE
.table
	border 1px solid color_2
	margin: 0 auto;
	// max-width: 520px
.table th
	font-size: 12px
	padding: 3px 10px
	color color_ii
	text-transform: uppercase;
	border-bottom: 1px solid color_2

.table td
	font-size: 12px
	color color_body
	padding: 3px 10px
	// max-width: 90px imp
	border-bottom: 1px solid color_2
